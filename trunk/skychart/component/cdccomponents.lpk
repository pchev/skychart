<?xml version="1.0"?>
<CONFIG>
  <Package Version="4">
    <Name Value="CDCcomponents"/>
    <AddToProjectUsesSection Value="True"/>
    <CompilerOptions>
      <Version Value="11"/>
      <SearchPaths>
        <UnitOutputDirectory Value="lib/$(TargetCPU)-$(TargetOS)-$(LCLWidgetType)"/>
      </SearchPaths>
      <Parsing>
        <SyntaxOptions>
          <UseAnsiStrings Value="False"/>
        </SyntaxOptions>
      </Parsing>
      <Other>
        <CompilerPath Value="$(CompPath)"/>
      </Other>
    </CompilerOptions>
    <Description Value="Requires packages for Cartes du Ciel"/>
    <Type Value="RunAndDesignTime"/>
    <RequiredPkgs Count="14">
      <Item1>
        <PackageName Value="MultiFramePackage"/>
        <DefaultFilename Value="multiframe/multiframepackage.lpk"/>
      </Item1>
      <Item2>
        <PackageName Value="bgrabitmappack"/>
        <DefaultFilename Value="bgrabitmap/bgrabitmappack.lpk"/>
      </Item2>
      <Item3>
        <PackageName Value="lazvo"/>
        <DefaultFilename Value="vo/lazvo.lpk"/>
      </Item3>
      <Item4>
        <PackageName Value="zoomimage"/>
        <DefaultFilename Value="zoomimage/zoomimage.lpk"/>
      </Item4>
      <Item5>
        <PackageName Value="XmlParser"/>
        <DefaultFilename Value="xmlparser/xmlparser.lpk"/>
      </Item5>
      <Item6>
        <PackageName Value="synapse"/>
        <DefaultFilename Value="synapse/source/lib/synapse.lpk"/>
      </Item6>
      <Item7>
        <PackageName Value="radec"/>
        <DefaultFilename Value="radec/radec.lpk"/>
      </Item7>
      <Item8>
        <PackageName Value="mrecsort"/>
        <DefaultFilename Value="mrecsort/mrecsort.lpk"/>
      </Item8>
      <Item9>
        <PackageName Value="libsql"/>
        <DefaultFilename Value="libsql/libsql.lpk"/>
      </Item9>
      <Item10>
        <PackageName Value="FCL"/>
        <MinVersion Major="1" Valid="True"/>
      </Item10>
      <Item11>
        <PackageName Value="enhedit"/>
        <DefaultFilename Value="enhedits/enhedit.lpk"/>
      </Item11>
      <Item12>
        <PackageName Value="downldialog"/>
        <DefaultFilename Value="downloaddialog/downldialog.lpk"/>
      </Item12>
      <Item13>
        <PackageName Value="CDCjdcalendar"/>
        <DefaultFilename Value="jdcalendar/cdcjdcalendar.lpk"/>
      </Item13>
      <Item14>
        <PackageName Value="uniqueinstance_package"/>
        <DefaultFilename Value="uniqueinstance/uniqueinstance_package.lpk"/>
      </Item14>
    </RequiredPkgs>
    <UsageOptions>
      <UnitPath Value="$(PkgOutDir)/"/>
    </UsageOptions>
    <PublishOptions>
      <Version Value="2"/>
      <IgnoreBinaries Value="False"/>
    </PublishOptions>
    <CustomOptions Items="ExternHelp" Version="2">
      <_ExternHelp Items="Count"/>
    </CustomOptions>
  </Package>
</CONFIG>
