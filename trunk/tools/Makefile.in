default: all

PREFIX=%PREFIX%

.PHONY: all
all: 
	#build data tools

ifeq ($(OS_TARGET),win32)
install: 
	.\install_data.cmd $(PREFIX) 
uninstall: 
	.\uninstall_data.cmd $(PREFIX) 
else
install: 
	./install_data.sh $(PREFIX) 
uninstall: 
	./uninstall_data.sh $(PREFIX) 
install_win: 
	./install_win_data.sh $(PREFIX)
uninstall_win: 
	./uninstall_win_data.sh $(PREFIX)
endif
