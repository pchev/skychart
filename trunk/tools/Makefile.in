default: all

PREFIX=%PREFIX%

.PHONY: all
all: 
	#build data tools

ifeq ($(OS_TARGET),win32)
install: 
	.\install_data.cmd $(PREFIX) 
uninstall: 
	.\uninstall_data.cmd $(PREFIX) 
else
ifeq ($(OS_TARGET),win64)
install: 
	.\install_data.cmd $(PREFIX) 
uninstall: 
	.\uninstall_data.cmd $(PREFIX) 
else
ifeq ($(OS_TARGET),darwin)
install: 
	./install_data_darwin.sh $(PREFIX) 
uninstall: 
	./uninstall_data_darwin.sh $(PREFIX) 
refresh_doc: 
	./refresh_wiki_doc.sh 
else
install: 
	./install_data.sh $(PREFIX) 
uninstall: 
	./uninstall_data.sh $(PREFIX) 
install_win: 
	./install_win_data.sh $(PREFIX)
uninstall_win: 
	./uninstall_win_data.sh $(PREFIX)
refresh_doc: 
	./refresh_wiki_doc.sh 
endif
endif
endif
                        
