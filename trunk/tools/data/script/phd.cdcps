event_52=2	        SetS('STR2',MsgVersion);
event_53=2	     end;
event_54=2	   end
event_55=2	   ;
event_56=2	end else begin
event_57=2	  p:=attrib.IndexOf('jsonrpc');   // Response to command
event_58=2	  if p>=0 then begin
event_59=2	     p:=attrib.IndexOf('id');
event_60=2	     rpcid:=value[p];
event_61=2	     i:=attrib.IndexOf('result');
event_62=2	     if i>=0 then rpcresult:=value[i]
event_63=2	     else begin
