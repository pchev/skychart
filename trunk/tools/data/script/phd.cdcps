event_79=2	     end
event_80=2	     else if rpcid='1002' then begin  // set_lock_shift_enabled
event_81=2	       if rpcresult='error' then memo_1.lines.add('set_lock_shift_enabled'+' '+rpcresult+' '+rpcerror);
event_82=2	     end
event_83=2	     else if rpcid='1003' then begin  // get_lock_shift_params
event_84=2	       if rpcresult='error' then
event_85=2	         memo_1.lines.add('get_lock_shift_params'+' '+rpcresult+' '+rpcerror)
event_86=2	       else begin
event_87=2	         i:=attrib.IndexOf('result.enabled');
event_88=2	         if i>=0 then begin
event_89=2	           edit_5.text:=value[i];
event_90=2	           if uppercase(trim(value[i]))='TRUE' then Button_4.Caption:='On'
event_91=2	              else Button_4.Caption:='Off';
event_92=2	         end
event_93=2	         else edit_5.text:='?';
event_94=2	         i:=attrib.IndexOf('result.axes');
event_95=2	         if i>=0 then edit_6.text:=value[i] else edit_6.text:='?';
event_96=2	         buf:='?';
event_97=2	         i:=attrib.IndexOf('result.rate.0');
event_98=2	         if i>=0 then buf:=value[i];
event_99=2	         i:=attrib.IndexOf('result.rate.1');
event_100=2	         if i>=0 then buf:=buf+','+value[i];
event_101=2	         edit_7.text:=buf;
event_102=2	         i:=attrib.IndexOf('result.units');
event_103=2	         if i>=0 then edit_8.text:=value[i] else edit_8.text:='?';
event_104=2	       end;
event_105=2	     end
event_106=2	     else if rpcid='1004' then begin  // get_lock_shift_enabled
event_107=2	       if rpcresult='error' then
event_108=2	         memo_1.lines.add('get_lock_shift_enabled'+' '+rpcresult+' '+rpcerror)
event_109=2	       else
event_110=2	         edit_5.text:=rpcresult;
event_111=2	         if uppercase(trim(rpcresult))='TRUE' then Button_4.Caption:='On'
event_112=2	            else Button_4.Caption:='Off';
event_113=2	     end;
event_114=2	  end;
event_115=2	end;
event_116=2	finally
event_117=2	 attrib.clear;
event_118=2	 value.clear;
event_119=2	end;
event_120=2	end;
