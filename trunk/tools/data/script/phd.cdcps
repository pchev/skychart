event_14=2	fullmsg:=txt;
event_15=2	JsontoStringlist(txt,attrib,value);
event_16=2	p:=attrib.IndexOf('Event');    // PHD events
event_17=2	if p>=0 then begin
event_18=2	   eventname:=value[p];
event_19=2	   if eventname='GuideStep' then edit_2.text:='Guiding'
event_20=2	   else if eventname='StartGuiding' then edit_2.text:='Guiding'
event_21=2	   else if eventname='GuidingStopped' then edit_2.text:='Stopped'
event_22=2	   else if eventname='StarSelected' then edit_2.text:='Star Selected'
event_23=2	   else if eventname='StarLost' then edit_2.text:='Star lost'
event_24=2	   else if eventname='Paused' then edit_2.text:='Paused'
event_25=2	   else if eventname='Resumed' then edit_2.text:='Resumed'
event_26=2	   else if eventname='LockPositionSet' then edit_2.text:='Lock Position Set'
event_27=2	   else if eventname='CalibrationComplete' then edit_2.text:='Calibration Complete'
event_28=2	   else if eventname='StarSelected' then edit_2.text:='Star Selected'
event_29=2	   else if eventname='StartCalibration' then edit_2.text:='Start Calibration'
event_30=2	   else if eventname='CalibrationFailed' then edit_2.text:='Calibration Failed'
event_31=2	   else if eventname='CalibrationDataFlipped' then edit_2.text:='Calibration Data Flipped'
event_32=2	   else if eventname='LoopingExposures' then edit_2.text:='Looping Exposures'
event_33=2	   else if eventname='LoopingExposuresStopped' then edit_2.text:='Looping Exposures Stopped'
event_34=2	   else if eventname='Settling' then edit_2.text:='Settling'
event_35=2	   else if eventname='SettleDone' then edit_2.text:='Settle Done'
event_36=2	   else if eventname='GuidingDithered' then edit_2.text:='Guiding Dithered'
event_37=2	   else if eventname='LockPositionLost' then edit_2.text:='Lock Position Lost'
event_38=2	   else if eventname='Alert' then edit_2.text:='Alert'
event_39=2	   else if eventname='AppState' then begin
event_40=2	     i:=attrib.IndexOf('State');
