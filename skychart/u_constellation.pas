function ConstellationCoord(ra,de: double): string;
// Based on VI/42 program.c
var i: integer;
const jd1875 = 2405889.5;
begin
  result:='';
  PrecessionFK5(jd2000,jd1875,ra,de);
  ra:=rad2deg*ra/15;
  de:=rad2deg*de;
  for i:=1 to lconstcoord do begin
     if (constcoord[i].de >  de) then
       continue ;
     if (constcoord[i].ra2 <= ra) then
       continue ;
     if (constcoord[i].ra1 >  ra) then
       continue ;
     if (ra >= constcoord[i].ra1) and (ra <  constcoord[i].ra2) and (constcoord[i].de <= de) then
       result:=constcoord[i].c
     else if(constcoord[i].ra2 <  ra) then
       continue
     else
       result:='';
     break;
  end;
end;

end.
