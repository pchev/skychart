[button_1]
s="var cm:TstringList;"
"    str:string;"
"    p: integer;"
"    plafov: array[0..9] of string;"
"begin"
"    plafov[0]:='0.7';"
"    plafov[1]:='0.015';"
"    plafov[2]:='0.015';"
"    plafov[3]:='0.7';"
"    plafov[4]:='0.01';"
"    plafov[5]:='0.02';"
"    plafov[6]:='0.02';"
"    plafov[7]:='0.005';"
"    plafov[8]:='0.005';"
"    plafov[9]:='0.00061';"
""
"    Memo_1.Clear;"
"    GetSL('STRL1',cm);"
""
"    if CheckList_3.Checked[0] then begin"
"      cm.clear;"
"      cm.add('SETFOV');"
"      cm.add(plafov[Combo_1.itemindex]);"
"      str:=cmd('',cm);"
"    end;"
""
"    cm.clear;"
"    cm.add('FIND');"
"    cm.add('8');"
"    cm.add(Combo_1.Text);"
"    str:=cmd('',cm);"
"    cm.clear;"
