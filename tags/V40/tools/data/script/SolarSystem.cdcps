[event_0]
s="var cm:TstringList;"
"    str,year: string;"
"    p: integer;"
"begin"
"  // Fill planet list"
"  Combo_1.Clear;"
"  Combo_1.Items.Add(rsSun);"
"  Combo_1.Items.Add(rsMercury);"
"  Combo_1.Items.Add(rsVenus);"
"  Combo_1.Items.Add(rsMoon);"
"  Combo_1.Items.Add(rsMars);"
"  Combo_1.Items.Add(rsJupiter);"
"  Combo_1.Items.Add(rsSaturn);"
"  Combo_1.Items.Add(rsUranus);"
"  Combo_1.Items.Add(rsNeptune);"
"  Combo_1.Items.Add(rsPluto);"
"  Combo_1.ItemIndex:=0;"
"  // planet zoom option"
"  CheckList_3.clear;"
"  CheckList_3.items.add(rszoom3);"
"  CheckList_3.Checked[0]:=true;"
"  // Get current year"
"  Edit_2.Text:='C/';"
"  GetSL('STRL1',cm);"
"  cm.clear;"
"  cm.add('GETDATE');"
"  str:=cmd('',cm);"
"  p:=pos('OK!',str);"
"  if p>0 then begin"
"    delete(str,1,p+2);"
"    p:=pos('-',str);"
"    year:=copy(str,1,p-1);"
"    Edit_2.Text:=year;"
"  end;"
"  // Fill comet list"
"  CheckList_1.Clear;"
"  cm.Clear;"
"  GetCometList(Edit_2.Text,1000,cm);"
"  CheckList_1.items.assign(cm);"
"  // Fill asteroid list"
"  Edit_3.Text:='*';"
"  CheckList_2.Clear;"
"  cm.Clear;"
"  GetAsteroidList('%',100,cm);"
"  CheckList_2.items.assign(cm);"
"  cm.clear;"
"end."


